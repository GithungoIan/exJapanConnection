extends base
block content
  main.main
    .card-container
      each vehicle in vehicles
        .card
          .card__header
            .card__picture
              .card__picture-overlay &nbsp;
              img.card__picture-img(src=`${vehicle.image}`, alt=`${vehicle.name}`)
                h3.heading-tertiary
                  span= vehicle.name
                  
            .card__details
              h4.card__sub-heading
